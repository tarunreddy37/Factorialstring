import java.util.Scanner;
import java.util.*;
class Factorialstring
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n,a;
System.out.print("enter the n value:  ");
n=sc.nextInt();
 a=fact(n);
System.out.print("factorial is:  "+a);
}
static int fact(int n )
{
if(n==0)
return 1;
else
return (n*fact(n-1));
}
}
